# Copy this file to terraform.tfvars and fill in your values.
#
# cp terraform.tfvars.example terraform.tfvars

aws_region             = "ap-southeast-2"
notification_email     = "your-email@example.com"

# Connect instance alias (must be globally unique, lowercase, no spaces)
connect_instance_alias = "caller-agent"

# Phone number settings
connect_phone_country  = "AU"
connect_phone_type     = "DID"

# Spam detection (optional â€” set to true and configure NumVerify to enable)
enable_spam_detection  = false
# numverify_secret_name = "caller-agent/numverify-api-key"
